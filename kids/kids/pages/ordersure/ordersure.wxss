/* pages/ordersure/ordersure.wxss */

.address {
  width: 100%;
  margin-top: 40rpx;
}

.address .content .title {
  color: #262626;
  font-size: 32rpx;
  border-bottom: 1px solid #dbdbdb;
  height: 86rpx;
  line-height: 86rpx;
  font-weight: bolder;
}

.address .content .con {
  height: 110rpx;
  line-height: 110rpx;
  display: flex;
  justify-content: space-between;
}

.address .content .con .con-l {
  color: #DFCB84;
  font-size: 30rpx;
}

.address .content .con .con-r {
  /* color: #9e9e9e; */
  width: 18rpx;
  height: 30rpx;
}
.address .content{
  padding:0 20rpx;
}
.address .content .con-has .con-has-t {
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #35373a;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.address .content .con-has .con-has-b {
  font-size: 28rpx;
  color: #35373a;
  margin-bottom: 40rpx;
}

.card {
  width: 100%;
  margin-top: 40rpx;
}

.card .con-title {
  width: 650rpx;
  height: 84rpx;
  line-height: 84rpx;
  margin: 0 auto;
  font-size: 32rpx;
  font-weight: bolder;
  border-bottom: 1px solid #dbdbdb;
  color: #262626;
}

.card .con-foot .con-foot-con {
  width: 650rpx;
  height: 120rpx;
  line-height: 120rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  border-top: 1px solid #dbdbdb;
  margin-top: 40rpx;
  font-size: 32rpx
}

.card .con-count .con-count-con {
  width: 650rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}

.card .con-count .con-count-con .con-count-con-l {
  width: 200rpx;
  height: 200rpx;
  flex-shrink: 0;
}

.card .con-count .con-count-con .con-count-con-l image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.card .con-count .con-count-con .con-count-con-r {
  width: 400rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.card .con-count .con-count-con .con-count-con-r .con-count-con-r-t {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #333;
  font-size: 28rpx;
  font-family:FZFangSong-Z02S!important;
}

.card .con-count .con-count-con .con-count-con-r .con-count-con-r-c {
  font-size: 24rpx;
  color: #9d9d9d;
  height: 36rpx;
  line-height: 36rpx;
}

.card .con-count .con-count-con .con-count-con-r .con-count-con-r-b {
  display: flex;
  justify-content: space-between;
  height: 36rpx;
  line-height: 36rpx;
}

.card .con-count .con-count-con .con-count-con-r .con-count-con-r-b .ds-l {
  font-size: 24rpx;
  color: #484a4c;
}

.card .con-count .con-count-con .con-count-con-r .con-count-con-r-b .ds-r {
  font-size: 18rpx;
  color: #9d9d9d;
}
