/* pages/shoppingcart/shoppingcart.wxss */

.container {
  padding-top: 60rpx;
  /* background: #fff; */
}

.shoppingcart-item {
  width: 100%;
  margin-bottom: 40rpx;
}

.shoppingcart-item .content .title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background:rgba(233, 203, 132, 0.3);
  border-radius: 10rpx 10rpx 0 0;
}

.shoppingcart-item .content .title {
  display: flex;
  justify-content: space-between;
}

.shoppingcart-item .content .title .tltle-l {
  margin-left: 30rpx;
  color: #484a4c;
  font-size: 36rpx;
   font-family:FZFangSong-Z02S!important;

}

.shoppingcart-item .content .title .title-r {
  margin-right: 30rpx;
  color: #484a4c;
  font-size: 20rpx;
}

.shoppingcart-item .content .main-con {
  /* padding-top: 30rpx; */
}

.shoppingcart-item .content .main-con .main-con-item {
  padding-left: 30rpx;
  margin-bottom: 30rpx;
}

.shoppingcart-item .content .main-con .main-con-item .main-con-item-con {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
  padding-right: 34rpx;
}

.shoppingcart-item .content .main-con .main-con-item .checkout-cir {
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #9d9d9d;
  border-radius: 50%;
  flex-shrink: 0;
  margin-right: 14rpx;
  margin-top: 80rpx;
  line-height: 27rpx;
}

.shoppingcart-item .content .main-con .main-con-item .checkout-cir-select {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  flex-shrink: 0;
  margin-right: 14rpx;
  margin-top: 80rpx;
  line-height: 27rpx;
  background: #DFCB84;
  text-align: center;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item {
  display: flex;
  justify-content: space-between;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item .goods-pic {
  width: 200rpx;
  height: 200rpx;
  flex-shrink: 0;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item .goods-pic image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des {
  width: 370rpx;
  margin-left: 14rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .titles {
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
   font-family:FZFangSong-Z02S!important;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .center {
  font-size: 24rpx;
  color: #9d9d9d;
}
.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .center-remark {
  font-size: 24rpx;
  color: red;
}


.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .bottom {
  display: flex;
  justify-content: space-between;
  height: 60rpx;
  line-height: 60rpx;
}

.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .bottom .bottom-l {
  font-size: 36rpx;
  color: #484a4c;
}
.shoppingcart-item .content .main-con .main-con-item .googs-item  .goods-des .bottom .bottom-l .bottom-l-icon{
  font-size: 28rpx;
  color: #484a4c;
}


.shoppingcart-item .content .main-con .main-con-item .delete {
  width: 120rpx;
  height: 260rpx;
}

.shoppingcart-item .content .main-con .main-con-item .delete .delete-con {
  width: 120rpx;
  height: 260rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #161C35;
  margin: 0 auto;
}

.shoppingcart-item .content .main-con .main-con-item .delete .delete-con .delete-con-icon {
  width: 28rpx;
  height: 28rpx;
}

.shoppingcart-item .content .main-con .main-con-item .delete .delete-con .delete-con-icon image {
  width: 100%;
  height: 100%;
}

.shoppingcart-item .content .main-con .main-con-item .delete .delete-con .text {
  font-size: 20rpx;
  color: #DFCB84;
  margin-top: 16rpx;
}

.line {
  margin: 0 20rpx;
  height: 1px;
  background: #dbdbdb;
}

.foots {
  margin: 0 0 0 20rpx;
}

.foots .foot-t {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #9d9d9d;
}

.foots .foot-b {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
}

.foots .foot-b .foot-b-l {
  /* font-size: 24rpx;
  color: #484A4C; */
}

.foots .foot-b .foot-b-l .icon {
  font-size: 28rpx;
  color: #484a4c;
}

.foots .foot-b .foot-b-l .price {
  font-size: 44rpx;
  color: #484a4c;
}

.foots .foot-b .foot-b-r {
  width: 240rpx;
  height: 80rpx;
  background: #DFCB84;
  border-radius: 10rpx 0 10rpx 0;
  text-align: center;
  line-height: 80rpx;
  color: #161C35;
  font-size: 30rpx;
}

.hasInven .hasInven-itrm-card {
  margin-bottom: 40rpx;
  /* background: #fff; */
}

.noInven-item{
  margin-bottom: 40rpx;
}
.noInven .content-item {
  position: relative;
  opacity: 0.7
}

.noInven .noInven-pop {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  background: #fff;
  opacity: 0.3;
  border-radius: 20rpx;
}

.noInven .noInven-title {
  display: flex;
  justify-content: center;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 160rpx;
  margin-bottom: 48rpx;
}

.noInven .noInven-title .noInven-title-line {
  width: 4rpx;
  height: 20rpx;
  background: #b0d7d7;
  transform: rotate(-45deg);
  margin-top: 10rpx;
}

.noInven .noInven-title .noInven-title-con {
  color: #484a4c;
  font-size: 24rpx;
  margin: 0 15rpx;
}

